{
  "name": "mouseandkeyboardrecorder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main/main.ts",
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "react",
    "typescript"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:5173/"
    }
  },
  "scripts": {
    "dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "vite build",
    "serve": "vite preview",
    "electron:dev": "VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "electron:build": "vite build && electron ."
  },
  "dependencies": {
    "@types/electron": "^1.6.10",
    "@types/lodash": "^4.17.7",
    "concurrently": "^8.2.2",
    "electron": "^31.2.1",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.25.0",
    "wait-on": "^7.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "typescript": "^5.2.2",
    "vite": "^5.3.4"
  }
}
